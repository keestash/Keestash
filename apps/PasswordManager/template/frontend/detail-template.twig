<div class="row" id="pwm__detail__part">
    <div class="col d-flex flex-column">
        <div class="col">
            <div class="row mt-4 mb-4">
                <div class="col-md-1 align-self-center">
                    <form id="my_form">
                        <div
                                class="avatar node-avatar"
                                style="width: 60px; height: 60px; border:none; background-image: url('data:image/svg+xml;base64,')"
                        ></div>
                        <input type="file" id="my_file" data-node-id="{{ node.id }}" style="display: none;"/>
                    </form>
                </div>
                <div class="col-md-4">
                    <div class="row">
                        <div class="col">
                            {{ node.name }}
                        </div>
                    </div>
                    {# <div class="row"> #}
                    {# <div class="col"> #}
                    {# {% if isFavorite == true %} #}
                    {# <i class="far fa-star favorite"></i> #}
                    {# {% else %} #}
                    {# <i class="far fa-star"></i> #}
                    {# {% endif %} #}
                    {# </div> #}
                    {# </div> #}
                </div>
            </div>
            <form id="tab__detail__wrapper">
                <div class="form-group">
                    <small class="form-text text-muted">{{ userNameLabel }}</small>
                    <input type="email" class="form-control" aria-describedby="emailHelp"
                           placeholder="{{ node.username }}" value="{{ node.username }}" id="pwm__login__username">
                </div>
                <div class="form-group">
                    <small class="form-text text-muted">{{ passwordLabel }}</small>
                    <div class="input-group">
                        <input type="password" class="form-control" id="pwm__login__password"
                               placeholder="{{ node.password.placeholder }}"
                               readonly value="{{ node.password.placeholder }}"
                               data-credential-placeholder="{{ node.password.placeholder }}"
                               data-credential-visible="0"
                               data-login-id="{{ node.id }}"
                               data-updated="0"
                        >
                        <div class="input-group-append" id="pwm__password__eye">
                            <div class="input-group-text">
                                <i class="fas fa-eye"></i>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <small class="form-text text-muted">{{ websiteLabel }}</small>
                    <div class="input-group">
                        <input type="url" class="form-control" aria-describedby="emailHelp"
                               placeholder="{{ node.url }}" value="{{ node.url|escape }}" id="pwm__login__website">
                        <div class="input-group-append" id="pwm__url__button">
                            <div class="input-group-text">
                                <i class="fas fa-external-link-alt"></i>
                            </div>
                        </div>

                    </div>
                </div>

                <ul class="nav nav-tabs customtab" role="tablist" id="detail__tabs">
                    {% if sharingEnabled %}
                        <li class="nav-item">
                            <a class="nav-link active"
                               data-toggle="tab"
                               href="#pwm__sharing"
                               role="tab"
                               data-tabname="shares"
                            ><span
                                        class="hidden-sm-up"><i class="fas fa-share-alt"></i></span> <span
                                        class="hidden-xs-down">{{ sharesLabel }}</span></a></li>
                    {% endif %}

                    <li class="nav-item">
                        <a class="nav-link"
                           data-toggle="tab"
                           href="#pwm__pw__generator"
                           role="tab"
                           data-tabname="pw-generator"
                        ><span
                                    class="hidden-sm-up"
                                    data-tabname="pw-generator"
                            >
                                <i class="fas fa-unlock"
                                   data-tabname="pw-generator"
                                >
                                </i>
                            </span>
                            <span
                                    class="hidden-xs-down"
                                    data-tabname="pw-generator"
                            >{{ pwGeneratorLabel }}
                            </span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link"
                           data-toggle="tab"
                           href="#comment"
                           role="tab"
                           data-tabname="comment"
                        ><span
                                    data-tabname="comment"
                                    class="hidden-sm-up"><i class="fas fa-comment-alt"
                                                            data-tabname="comment"></i></span> <span
                                    class="hidden-xs-down" data-tabname="comment">{{ commentLabel }}</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link"
                           data-toggle="tab"
                           href="#attachment"
                           role="tab"
                           data-tabname="attachments"
                        ><span data-tabname="attachments"
                               class="hidden-sm-up"><i data-tabname="attachments" class="fas fa-file"></i></span> <span
                                    class="hidden-xs-down" data-tabname="attachments">{{ attachmentsLabel }}</span></a>
                    </li>

                </ul>
                <div class="tab-content" id="pwm__tab__content">
                    <div class="tab-pane active" id="pwm__sharing" role="tabpanel">
                        {% if sharingEnabled %}

                        <div class="container mt-3 ">
                            <div class="input-group" id="searchFollowee">
                                <input
                                        placeholder="{{ sharePlaceholder }}"
                                        type="text"
                                        class="form-control form-control-sm prompt typeahead"
                                >
                            </div>

                            <div class="container mt-3">
                                <div class="row justify-content-between">
                                    <div class="col-3 ">
                                        <button class="btn btn-primary btn-circle btn-sm" id="share_publicly">
                                            <i class="fas fa-share"></i>
                                        </button>
                                        {{ sharePublicly }}
                                    </div>
                                    {% if sharedPublicly == true %}
                                    <div class="col-2  d-flex flex-column align-items-end" id="copy_link_button">
                                        {% else %}
                                        <div class="col-2 copy__link__invisible" id="copy_link_button">
                                            {% endif %}
                                            <button class="btn btn-secondary btn-circle btn-sm"
                                                    id="ii__copy__link__button"
                                            >
                                                <i class="fas fa-copy"></i>
                                            </button>

                                        </div>
                                    </div>
                                </div>

                                <div class="results mt-3 rounded border tab_result_box" id="share__results">
                                    <ul class="list-group list-group-flush"></ul>
                                </div>

                            </div>

                            {% endif %}
                        </div>

                        <div class="tab-pane" id="pwm__pw__generator" role="tabpanel">
                            <div class="container mt-3 ">
                                <div class="row">

                                    <div class="col">
                                        <div class="input-group">
                                            <input placeholder="{{ passwordGeneratorPlaceholder }}"
                                                   id="pwm__generator__input"
                                                   type="text"
                                                   class="form-control form-control-sm">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col  mt-1">
                                        <div class="container">
                                            <div class="row justify-content-between">
                                                <div class="col-4">
                                                    <label>{{ qualityLabel }}:</label>
                                                    <span id="pwm__quality__label">{{ qualityValue }}</span>
                                                </div>
                                                <div class="col-4">
                                                    <label>{{ entropyLabel }}:</label>
                                                    <span id="pwm__entropy__value">{{ entropyValue }}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <small>{{ characterTypes }}</small>

                                <div class="border rounded">
                                    <div class="row">
                                        <div class="col">
                                            <div class="ui radio checkbox">
                                                <input type="checkbox" name="pwm__upper__case" id="pwm__upper__case">
                                                <label>{{ upperCaseLabel }}</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col">
                                            <div class="ui radio checkbox">
                                                <input type="checkbox" name="pwm__lower__case" id="pwm__lower__case">
                                                <label>{{ lowerCaseLabel }}</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col">
                                            <div class="ui radio checkbox">
                                                <input type="checkbox" name="pwm__digit" id="pwm__digit">
                                                <label>{{ digitLabel }}</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col">
                                            <div class="ui radio checkbox">
                                                <input type="checkbox" name="pwm__special__character"
                                                       id="pwm__special__character">
                                                <label>{{ specialCharacterLabel }}</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col" id="pwm__range__input__wrapper">
                                        <input
                                                class="form-control-range"
                                                id="pwm__range__input"
                                                type="range"
                                                value="{{ defaultValue }}"
                                                min="{{ minPasswordCharacters }}"
                                                max="{{ maxPasswordCharacters }}"
                                                step="{{ stepPasswordCharacters }}"
                                        >
                                    </div>
                                </div>
                                <div class="row d-flex justify-content-between">
                                    <div class="col">
                                        <span>{{ minPasswordCharacters }}</span>
                                    </div>
                                    <div class="col">
                                        <span id="pwm__character__count">{{ defaultValue }}</span>
                                    </div>
                                    <div class="col">
                                        <span>{{ maxPasswordCharacters }}</span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <button class="btn btn-primary" id="pwm__use__button">{{ usePassword }}</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane" id="comment" role="tabpanel">
                            <div class="results mt-3 rounded border tab_result_box" id="comment__result">
                                <ul class="list-group list-group-flush">
                                    <div class="spinner-border" role="status"></div>
                                </ul>
                            </div>

                            <form id="pwm__new__comment__form">
                                <div class="form-group">
                                    <label for="pwm__notes__note__area">{{ commentHeader }}</label>
                                    <textarea class="form-control" id="pwm__notes__note__area"
                                              rows="3">{{ notes }}</textarea>
                                </div>

                                <button
                                        id="pwm__notes__note"
                                        class="btn btn-primary"
                                        data-node-id="{{ node.id }}"
                                        data-user-id="{{ node.user_id }}"
                                >{{ addComment }}</button>
                            </form>
                        </div>
                        <div class="tab-pane" id="attachment" role="tabpanel">
                            <div class="results mt-3 rounded border tab_result_box" id="attachments__results">
                                <ul class="list-group list-group-flush">
                                    <div class="spinner-border" role="status"></div>
                                </ul>
                            </div>

                            <hr>
                            <div class="form-group">
                                <label for="exampleFormControlFile1">{{ attachmentsLabel }}</label>
                                <input type="file" class="form-control-file" id="pwm__attachments__input" multiple>
                            </div>

                        </div>
                    </div>
                    {# <div class="container"> #}
                    {# <div class="row"> #}
                    {# <div class="col-2"> #}
                    {# {{ createdAt }} #}
                    {# </div> #}
                    {# <div class="col-3"> #}
                    {# {{ createDate }} #}
                    {# </div> #}
                    {# </div> #}
                    {# <div class="row"> #}
                    {# <div class="col-2"> #}
                    {# {{ updatedAt }} #}
                    {# </div> #}
                    {# <div class="col-3"> #}
                    {# {{ updateDate }} #}
                    {# </div> #}
                    {# </div> #}
                    {# </div> #}
            </form>


        </div>
    </div>
</div>

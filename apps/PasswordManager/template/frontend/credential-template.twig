<div class="text-muted pwm__row container-fluid"
     data-id="{{ id }}"
     data-type="{{ type }}">

    <div class="d-flex justify-content-start align-items-start flex-grow-1 row">

        <div class="col">
            <div
                    class="flex-grow-1 "
                    {% if type == 'credential' %}
                        style="width: 30px; height: 30px; border:none; background-image: url('data:image/svg+xml;base64,')"
                    {% else %}
                        style="width: 30px; height: 30px; border:none; background-image: url('data:image/svg+xml;base64,')"
                    {% endif %}
            ></div>

            {% if isShared == true %}
                <i class="fas fa-share" title="{{ tooltip }}"></i>
            {% endif %}
        </div>

        <div class="col-7">
            <div class="row">
                <div class="col cropped" title="{{ name }}">
                    {{ name }}
                    <br>
                    <small>{{ info }}</small>
                </div>
            </div>
        </div>

        <div class="col d-flex justify-content-center align-items-center context-menu-menu-badge">
            <div class="row">
                <div class="col">
                    <i class="fas fa-ellipsis-h fa"></i>
                </div>
            </div>
        </div>
    </div>
</div>

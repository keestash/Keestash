{
  "name": "dogano/keestash",
  "type": "project",
  "license": "GNU Affero General Public License",
  "authors": [
    {
      "name": "Dogan Ucar",
      "email": "dogan@dogan-ucar.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "doganoo/php-util": "0.3.12",
    "doganoo/php-algorithms": "0.0.27",
    "doganoo/simple-rbac": "1.5.0",
    "symfony/routing": "4.2.5",
    "symfony/http-foundation": "4.2.12",
    "symfony/webpack-encore-pack": "1.0.3",
    "php-di/php-di": "6.0.7",
    "twig/twig": "^2.0",
    "robmorgan/phinx": "0.11.1",
    "filp/whoops": "2.3.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "xobotyi/php-mime-type": "1.2.0",
    "ext-openssl": "*",
    "phpmailer/phpmailer": "^6.0",
    "ext-posix": "*",
    "symfony/mime": "^4.3",
    "ocramius/package-versions": "1.4.0",
    "ext-sqlite3": "*",
    "keepassphp/keepassphp": "v1.1.0-RC2"
  },
  "autoload": {
    "psr-4": {
      "Keestash\\": "lib/private/",
      "KSP\\": "lib/public/",
      "KST\\": "test/",
      "KSA\\": "apps/"
    }
  },
  "scripts": {
    "migrate-apps": "./bin/migration/migrate.php -m=app",
    "migrate-instance": "./bin/migration/migrate.php -m=instance",
    "phpstan": "./vendor/bin/phpstan analyse lib/ test/",
    "clear-dist": "php bin/clear_dist.php"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "phpstan/phpstan": "^0.11.16"
  }
}

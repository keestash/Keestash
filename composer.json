{
  "name": "keestash/server",
  "type": "project",
  "description": "Open Source Password Safe",
  "homepage": "https://keestash.com",
  "readme": "README.md",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Dogan Ucar",
      "email": "dogan@dogan-ucar.de"
    }
  ],
  "require": {
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysql": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "ext-sqlite3": "*",
    "ext-zip": "*",
    "doctrine/dbal": "^2.10",
    "doganoo/backgrounder": "^3.1",
    "doganoo/di-services": "^0.0",
    "doganoo/php-algorithms": "^1.0",
    "doganoo/php-util": "^0.3",
    "doganoo/simple-rbac": "^1.6",
    "filp/whoops": "^2.7",
    "giggsey/libphonenumber-for-php": "^8.12",
    "laminas/laminas-uri": "^2.7",
    "laminas/laminas-validator": "^2.13",
    "php-di/php-di": "^6.2",
    "phpmailer/phpmailer": "^6.1",
    "robmorgan/phinx": "^0.11",
    "scssphp/scssphp": "^1.2",
    "symfony/console": "^4.4",
    "symfony/http-foundation": "^4.2",
    "symfony/mime": "^4.4",
    "symfony/routing": "^4.4",
    "twig/twig": "^2.13",
    "twitter/bootstrap": "^4.5",
    "xobotyi/php-mime-type": "^1.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.8",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^9.3",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Keestash\\": "lib/private/",
      "KSP\\": "lib/public/",
      "KST\\": "test/",
      "KSA\\": "apps/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": " cd vendor/bin && ./phpunit ../../test/",
    "phpstan": "cd vendor/bin && ./phpstan analyse -c ../../config/phpstan/phpstan.neon ../../apps/ ../../lib --level 8 --memory-limit=2G"
  },
  "support": {
    "email": "dogan@dogan-ucar.de",
    "issues": "https://github.com/keestash/server/issues",
    "source": "https://github.com/keestash/server"
  }
}
